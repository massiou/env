    <extensionMap>
      <mapping ext="init" type="PLAIN_TEXT" />
      <mapping ext="conf" type="PLAIN_TEXT" />
      <mapping ext="yaml" type="SnakeYAML" />
      <mapping ext="yml" type="SnakeYAML" />
      <removed_mapping ext="yml" approved="true" type="YAML_EX" />
      <removed_mapping ext="yaml" approved="true" type="YAML_EX" />
    </extensionMap>
  </component>
</application>